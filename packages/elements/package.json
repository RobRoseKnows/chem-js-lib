{
  "name": "@chemistry/elements",
  "version": "1.3.3",
  "description": "Chemical elements information",
  "author": {
    "name": "Volodymyr Vreshch",
    "url": "http://vreshch.com/"
  },
  "license": "MIT",
  "keywords": [
      "elements",
      "periodic table",
      "cheminformatics",
      "Van der Waals radius",
      "covalent radius",
      "chemistry"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
      "type": "git",
      "url": "https://github.com/chemistry/chem-js-lib.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "private": false,
  "main": "dist/elements.js",
  "typings": "dist/elements.d.ts",
  "scripts": {
      "build": "npm run clean && npm run build:prod",
      "build:prod": "webpack-cli --config etc/webpack.config.base.js --mode production",
      "clean": "rimraf dist/* coverage/*",
      "test": "rimraf coverage/* && npm run test:node && npm run test:browser && npm run coverage",
      "test:node": "ts-node node_modules/jasmine/bin/jasmine.js JASMINE_CONFIG_PATH=etc/jasmine.json",
      "coverage": "ts-node node_modules/istanbul/lib/cli.js cover -e .ts  -x \"*.d.ts\" -x \"*.spec.ts\" node_modules/jasmine/bin/jasmine.js -- JASMINE_CONFIG_PATH=etc/jasmine.json",
      "test:browser": "karma start --single-run --no-auto-watch etc/karma.conf.js",
      "tdd": "nodemon -e ts -w ./src -x ts-node node_modules/jasmine/bin/jasmine.js JASMINE_CONFIG_PATH=etc/jasmine.json",
      "lint": "tslint -c tslint.json -p tsconfig.json 'src/**/*.ts'",
      "lintfix": "tslint -c tslint.json -p tsconfig.json 'src/**/*.ts' --fix",
      "codecov": "codecov -t f7e73766-d8e8-406d-bc14-b15e6f26434f < coverage/lcov.info",
      "do-publish": "npm run build && npm publish"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jasmine": "2.8.8",
    "awesome-typescript-loader": "5.2.1",
    "codecov": "3.1.0",
    "cross-env": "5.2.0",
    "istanbul": "1.1.0-alpha.1",
    "jasmine": "3.2.0",
    "jasmine-core": "3.2.1",
    "karma": "3.0.0",
    "karma-jasmine": "1.1.2",
    "karma-mocha-reporter": "2.2.5",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-webpack": "3.0.4",
    "phantomjs-prebuilt": "2.1.16",
    "loader-utils": "1.1.0",
    "nodemon": "1.18.4",
    "rimraf": "2.6.2",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.0.3",
    "webpack": "4.18.0",
    "webpack-cli": "3.1.0"
  }
}

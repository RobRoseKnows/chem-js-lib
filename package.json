{
    "name": "@chemistry/chem-js-lib",
    "version": "2.2.0",
    "description": "Open Cheminformatics Libraries",
    "author": {
        "name": "Volodymyr Vreshch",
        "url": "http://vreshch.com/"
    },
    "license": "MIT",
    "private": true,
    "keywords": [
        "cheminformatics",
        "chemistry",
        "javascript"
    ],
    "workspaces": [
        "packages/*"
    ],
    "engines": {
        "node": ">=6.0.0"
    },
    "devEngines": {
        "node": ">=8.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/chemistry/chem-js-lib.git"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "scripts": {
        "clean": "lerna clean",
        "bootstrap": "lerna bootstrap",
        "build": "lerna run build",
        "test": "lerna run test",
        "updated": "lerna updated",
        "lint": "tslint -c tslint.json -p tsconfig.json 'packages/**/src/**/*.ts'",
        "lintfix": "tslint -c tslint.json -p tsconfig.json 'packages/**/src/**/*.ts' --fix",
        "do-publish": "npm run clean && npm run bootstrap && npm run build && lerna publish"
    },
    "dependencies": {},
    "devDependencies": {
        "lerna": "3.4.3",
        "@types/jasmine": "2.8.11",
        "awesome-typescript-loader": "5.2.1",
        "codecov": "3.1.0",
        "cross-env": "5.2.0",
        "istanbul": "1.1.0-alpha.1",
        "jasmine": "3.3.0",
        "jasmine-core": "3.3.0",
        "karma": "3.1.1",
        "karma-jasmine": "1.1.2",
        "karma-mocha-reporter": "2.2.5",
        "karma-phantomjs-launcher": "1.0.4",
        "karma-webpack": "3.0.5",
        "phantomjs-prebuilt": "2.1.16",
        "loader-utils": "1.1.0",
        "nodemon": "1.18.6",
        "rimraf": "2.6.2",
        "ts-node": "7.0.1",
        "tslint": "5.11.0",
        "typescript": "3.1.6",
        "webpack": "4.25.1",
        "webpack-cli": "3.1.2"
    }
}
